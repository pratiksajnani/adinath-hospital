{
  "source": {
    "include": ["js/"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(node_modules|tests)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "destination": "./docs/api",
    "recurse": true,
    "readme": "README.md"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  }
}
